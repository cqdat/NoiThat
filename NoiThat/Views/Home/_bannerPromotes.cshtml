@using NoiThat.Utilities
@using NoiThat.Models.DataModels
@model IEnumerable<Advertise>
<div class="offer-banner-section">
    <div class="container">
        <div class="row">
            @foreach (var i in Model)
            {
                <div class="col-lg-4 col-xs-12 col-md-4 col-sm-4 wow">
                    <a href="#"><img alt="@i.AdvertiseName" src="@WebConstants.ImgNewsHost/@i.AdvertiseImage"></a>
                </div>
            }


        </div>
    </div>
</div>